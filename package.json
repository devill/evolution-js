{
  "name": "evolution-js",
  "version": "0.1.0",
  "description": "Simulation of life right in your browser!",
  "main": "main.js",
  "scripts": {
    "start": "node server/index.js",
    "build": "gulp build-client",
    "watch": "gulp",
    "test": "npm run test-server; npm run test-client",
    "test-server": "DATABASE_URL=\"postgres://postgres_user:postgres_pw@localhost:9832/postgres_db\" mocha test/server/app.spec.js",
    "patch-db": "db-migrate -e dev up && db-migrate -e test up",
    "test-client": "gulp test-client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devill/evolution-js.git"
  },
  "keywords": [
    "life",
    "evolution",
    "creature",
    "genetic",
    "algorithm",
    "neural",
    "network"
  ],
  "author": "devill",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devill/evolution-js/issues"
  },
  "homepage": "https://github.com/devill/evolution-js#readme",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "chai": "^3.4.1",
    "db-migrate": "^0.9.23",
    "gulp": "^3.9.0",
    "karma": "^0.13.15",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sinon": "^1.0.4",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.19",
    "sinon": "^1.17.2",
    "superagent": "^1.6.1",
    "supertest": "^1.1.0",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "co": "4.6.0",
    "co-pg": "1.3.1",
    "koa": "1.1.2",
    "koa-bodyparser": "2.0.1",
    "koa-logger": "1.3.0",
    "koa-route": "2.4.2",
    "koa-static": "1.5.2",
    "pg": "4.4.3",
    "vis": "4.10.0"
  }
